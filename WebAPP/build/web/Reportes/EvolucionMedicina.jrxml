	<parameter name="cedula" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select p.paciente_dni, (p.paciente_primer_nombre || ' ' || p.paciente_segundo_nombre) as nombres,
(p.paciente_apellido_paterno || ' ' || p.paciente_apellido_materno) as apellidos,
TO_CHAR (mg.fecha:: DATE, 'dd-mm-yyyy')as fecha, TO_CHAR (mg.hora:: TIME, 'HH24:MI')as HORA,
string_agg(dr.medicamento, '' || CHR(13))as medicamento, string_agg(dr.indicaciones, ''|| CHR(13)) as indicacion,
('PA: '|| s.pa_sistolica || '/' || s.pa_diastolica || CHR(13) || 'FC: '|| s.frecuencia_cardiaca || 'x' || CHR(13) ||
	   'FR: ' || s.fr || 'x'|| CHR(13) || 'SPO: '|| s.saturacion || '%' || CHR(13) || 'T: '|| s.temperatura || 'ºC'|| CHR(13) ||
	   'Peso: ' || s.peso || 'kg' || CHR(13) || 'Talla: ' || s.estatura || 'cm' || CHR(13) || 'HEA= ' || mg.notas) as evolucion
from medicinageneral as m, signos_vitales as s, paciente as p, mg_seguimiento as mg, recetamedica as r, detalle_recetamedica as dr
where m.signos_id=s.signos_id and m.paciente_id=p.paciente_id and mg.idmedicinageneral=m.idmedicinageneral and
r.id_recetamedica=mg.id_recetamedica and dr.id_recetamedica=r.id_recetamedica and p.paciente_dni= $P{cedula}
group by 1, s.pa_sistolica, s.pa_diastolica, s.frecuencia_cardiaca, s.fr, s.saturacion, s.temperatura, s.peso,
s.estatura, mg.notas, mg.hora, mg.fecha, p.paciente_primer_nombre, p.paciente_segundo_nombre, paciente_apellido_paterno,
p.paciente_apellido_materno, p.paciente_dni]]>
	</queryString>
